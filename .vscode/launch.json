{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        },
        {
            "name": "Listen for XDebug on Homestead",
            "type": "php",
            "request": "launch",
            "pathMappings": {
                "/home/vagrant/proyectos/sysCorpo/": "${workspaceRoot}"
            },
            "port": 9000
        },
        {
            "name": "Listen for XDebug on Homestead proyectoFran",
            "type": "php",
            "request": "launch",
            "pathMappings": {
                "/home/vagrant/Ramiro/proyectoFran/": "${workspaceRoot}"
            },
            "port": 9000
        },
        {
            "name": "Launch currently open script 2",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true,
            "cwd": "${fileDirname}",
            "port": 9000,
            "runtimeExecutable": "C:php\\php.exe"
        }
    ]
}